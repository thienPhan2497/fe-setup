{
	"endOfLine": "lf",
	"printWidth": 100,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 3,
	"trailingComma": "all",
	"useTabs": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"jsxSingleQuote": true
}
